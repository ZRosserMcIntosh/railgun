# Rail Gun Desktop - Environment Variables
# Copy to .env.local for local development overrides
# Copy to .env.production for production builds

# API URL (without trailing slash)
# Development: http://localhost:3001
# Production: https://api.railgun.app
VITE_API_URL=http://localhost:3001

# WebSocket URL (without trailing slash or /ws path)
# Development: http://localhost:3001
# Production: https://api.railgun.app
VITE_WS_URL=http://localhost:3001

# Update server URL for auto-updates
VITE_UPDATE_URL=https://update.railgun.app

# Environment: development, staging, production
VITE_ENV=development
